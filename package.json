    "naming",
    "block",
    "element",
    "className"
  ],
  "author": "Anton Winogradov <winogradovaa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/awinogradov/easy-bem-naming/issues"
  },
  "homepage": "https://github.com/awinogradov/easy-bem-naming#readme",
  "dependencies": {
    "@bem/sdk.naming.entity.stringify": "0.2.1",
    "@bem/sdk.naming.presets": "0.0.4"
  },
  "devDependencies": {
    "jest": "^20.0.0",
    "typescript": "^2.5.3"
  },
  "jest": {
    "moduleNameMapper": {
      "easy-bem-naming": "<rootDir>/src/index.ts"
